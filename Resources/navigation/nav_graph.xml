<?xml version="1.0" encoding="utf-8" ?>


<!--Navigation element is the root. It contains destinations (fragments and activities)
which are connected to each other by actions. -->

<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            app:startDestination="@+id/navigation_todo">
    
	
	<fragment 
    android:id="@+id/navigation_todo"
    android:name="com.companyname.functional_bubble.NET.Fragments.TodoBase"
    android:label="@string/title_home" />
	
	<fragment 
    android:id="@+id/navigation_shop"
    android:name="com.companyname.functional_bubble.NET.Fragments.ShopBase"
    android:label="@string/title_dashboard" />
    
	<fragment 
    android:id="@+id/navigation_penguin"
    android:name="com.ThreePenguins.functional_bubble.NET.Fragments.PenguinBase"
    android:label="@string/title_notifications" />

</navigation>
